{
  "name": "eta-orionis/composer-json-manipulator",
  "description": "Package to load, merge and save composer.json file(s). Cleaned up from symplify/composer-json-manipulator",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "EtaOrionis\\ComposerJsonManipulator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EtaOrionis\\ComposerJsonManipulator\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
